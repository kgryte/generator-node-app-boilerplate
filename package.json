{
  "name": "generator-node-app-boilerplate",
  "version": "0.0.0",
  "description": "A Yeoman generator for creating a new boilerplate Node application.",
  "author": {
    "name": "Athan Reines",
    "email": "kgryte@gmail.com",
    "url": "https://github.com/kgryte"
  },
  "contributors": [
    {}
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coveralls/coverage --report lcovonly -- -R spec && cat ./reports/coveralls/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coveralls"
  },
  "main": "app/index.js",
  "files": [
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kgryte/generator-node-app-boilerplate.git"
  },
  "keywords": [
    "yeoman-generator",
    "kgryte",
    "node",
    "boilerplate",
    "server",
    "app",
    "application"
  ],
  "bugs": {
    "url": "https://github.com/kgryte/generator-node-app-boilerplate/issues"
  },
  "dependencies": {
    "chalk": "^1.0.0",
    "shelljs": "^0.4.0",
    "yeoman-generator": "^0.18.9",
    "yosay": "^1.0.0"
  },
  "devDependencies": {
    "mocha": "2.x.x",
    "chai": "3.x.x",
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x"
  },
  "peerDependencies": {
    "yo": ">=1.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
