{
  "name": "<%= name %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "author": {
    "name": "<%= author %>",
    "email": "<%= email %>"
  },
  "contributors": [
    {
      "name": "<%= author %>",
      "email": "<%= email %>"
    }
  ],
  "bin": {
  	"<%= name %>": "./bin/server"
  },
  "scripts": {
    "start": "node ./bin/server | ./node_modules/.bin/bunyan",
    "test": "./node_modules/.bin/mocha",
    "test-cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coveralls/coverage --report lcovonly -- -R spec && cat ./reports/coveralls/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coveralls"
  },
  "main": "",
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= repo %>.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/<%= repo %>/issues"
  },
  "dependencies": {
    "body-parser": "^1.12.0",
    "bootable": "^0.2.4",
    "bunyan": "^1.3.4",
    "compression": "^1.4.1",
    "connect-middleware-monitor": "^1.0.3",
    "express": "^4.12.2",
    "monitor-plugin-os": "^1.0.0",
    "monitor-plugin-process": "^1.0.0",
    "node-uuid": "^1.4.3",
    "pkginfo": "^0.3.0",
    "response-time": "^2.3.0",
    "topical": "^1.1.1"
  },
  "devDependencies": {
    "chai": "2.x.x",
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.5",
    "jshint": "2.x.x",
    "jshint-stylish": "^1.0.0",
    "mocha": "2.x.x",
    "proxyquire": "^1.4.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ]
}
